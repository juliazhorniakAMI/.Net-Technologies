@model List<TaxiServiceBD.Models.Category>
<table class="table">
    <thead>
        <tr>
            <th>
              Id
            </th>

            <th>
                FullName
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.FullName)
            </td>

        </tr>
        }
    </tbody>
</table>
